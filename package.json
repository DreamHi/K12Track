{
  "name": "template-react-app",
  "version": "0.2.0",
  "description": "基于React的单页面应用模版",
  "main": "index.js",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test --env=node --coverage",
    "predeploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DreamHi/template-react-app.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DreamHi/template-react-app/issues"
  },
  "homepage": "https://github.com/DreamHi/template-react-app#readme",
  "dependencies": {
    "antd": "3.21.4",
    "axios": "0.19.0",
    "history": "4.9.0",
    "js-audio-recorder": "0.3.1",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "query-string": "6.8.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-intl": "3.1.8",
    "react-recorder": "^1.0.0",
    "react-router-dom": "5.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.12.0",
    "customize-cra": "^0.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^3.1.1",
    "react-styleguidist": "^9.1.14",
    "react-test-renderer": "^16.9.0",
    "source-map-explorer": "^2.0.1",
    "webpack-ant-icon-loader": "^1.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix ./src --ext .jsx",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
